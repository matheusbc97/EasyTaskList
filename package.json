{
  "name": "EasyTaskList",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:build": "cd android/ && ./gradlew assembleRelease && cd ..",
    "android:clean": "cd android/ && ./gradlew clean && cd ..",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "update-rn": "react-native upgrade",
    "update-libraries": "yarn upgrade-interactive --latest",
    "test-e2e":
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "3.0.9",
    "@react-native-community/hooks": "2.6.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-firebase/app": "12.9.1",
    "@react-native-firebase/auth": "12.9.1",
    "@react-native-firebase/firestore": "12.9.1",
    "@react-navigation/bottom-tabs": "5.11.11",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.5",
    "@reduxjs/toolkit": "1.6.1",
    "@unform/core": "2.1.6",
    "@unform/mobile": "2.1.6",
    "date-fns": "2.17.0",
    "detox": "^19.4.2",
    "i18next": "^20.4.0",
    "lodash": "4.17.21",
    "polished": "4.1.3",
    "react": "17.0.1",
    "react-i18next": "11.11.4",
    "react-native": "0.64.1",
    "react-native-calendars": "1.1249.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-linear-gradient": "2.5.6",
    "react-native-modal": "11.6.1",
    "react-native-modal-datetime-picker": "9.1.0",
    "react-native-paper": "4.9.2",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.17.1",
    "react-native-vector-icons": "8.1.0",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "styled-components": "5.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/runtime": "7.12.5",
    "@react-native-community/eslint-config": "2.0.0",
    "@testing-library/react-native": "^8.0.0",
    "@types/jest": "24.0.24",
    "@types/lodash": "4.14.171",
    "@types/react-native": "0.63.50",
    "@types/react-native-calendars": "1.505.0",
    "@types/react-native-vector-icons": "6.4.8",
    "@types/react-redux": "7.1.18",
    "@types/react-test-renderer": "16.9.3",
    "@types/styled-components-react-native": "5.1.1",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.20.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "jest-circus": "^27.4.6",
    "metro-react-native-babel-preset": "0.64.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "reactotron-react-native": "5.0.0",
    "typescript": "4.1.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native|@react-navigation/.*)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./jest/setup.js"
    ]
  }
}
